{
  "name": "scrobbler-for-discogs-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|scrobbler-for-discogs-libs|immer|@reduxjs/toolkit|react-redux)"
    ],
    "moduleNameMapper": {
      "^@libs$": "<rootDir>/../libs/src/index.ts",
      "^@libs/(.*)$": "<rootDir>/../libs/src/$1",
      "^scrobbler-for-discogs-libs$": "<rootDir>/../libs/src/index.ts"
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
      "^.+\\.tsx?$": [
        "babel-jest",
        {
          "configFile": "./babel.config.js"
        }
      ]
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts?(x)",
      "**/*.test.ts?(x)"
    ],
    "roots": [
      "<rootDir>/src",
      "<rootDir>/../libs/src"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/slider": "^5.1.2",
    "@react-navigation/bottom-tabs": "^7.9.1",
    "@react-navigation/native": "^7.1.27",
    "@react-navigation/native-stack": "^7.9.1",
    "@reduxjs/toolkit": "^2.11.2",
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "expo": ">=54.0.0",
    "expo-auth-session": "^7.0.10",
    "expo-constants": "~18.0.13",
    "expo-crypto": "^15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-secure-store": "^15.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "^15.0.10",
    "react": "19.2.3",
    "react-native": "0.81.5",
    "react-native-css-interop": "^0.2.1",
    "react-native-dotenv": "^3.4.11",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.2.3",
    "babel-preset-expo": "^54.0.9",
    "expo": "^54.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.16",
    "react-test-renderer": "^19.2.3",
    "typescript": "~5.9.2"
  },
  "peerDependencies": {
    "expo": ">=54.0.0"
  },
  "peerDependenciesMeta": {
    "expo": {
      "optional": true
    }
  },
  "private": true
}
